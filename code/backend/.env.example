# JWT Configuration
JWT_SECRET=your-secure-jwt-secret-here

# Firebase Service Account Key Path (for FCM v1 API)
FIREBASE_SERVICE_ACCOUNT_KEY_PATH=/path/to/your/firebase-service-account-key.json

# Gmail SMTP Configuration
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USERNAME=your-actual-email@gmail.com
SMTP_PASSWORD=your-actual-app-password

# Google Maps API
GOOGLE_MAPS_API_KEY=your-actual-google-maps-api-key-here

# Database Credentials
USER_DB_PASSWORD=your-secure-user-db-password
ORDER_DB_PASSWORD=your-secure-order-db-password
TRACKING_DB_PASSWORD=your-secure-tracking-db-password
NOTIFICATION_DB_PASSWORD=your-secure-notification-db-password
CAMPAIGN_DB_PASSWORD=your-secure-campaign-db-password

# RabbitMQ Credentials
RABBITMQ_PASSWORD=your-secure-rabbitmq-password
